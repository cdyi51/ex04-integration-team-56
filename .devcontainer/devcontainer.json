{
    "dockerComposeFile": "docker-compose.yml",
    "workspaceFolder": "/workspace",
    "service": "httpd",
    "remoteUser": "vscode",
    "forwardPorts": [
        4200,
        8000,
        8080
    ],
    "customizations": {
        "vscode": {
            "extensions": [
                "ms-python.vscode-pylance",
                "GitHub.vscode-pull-request-github",
                "angular.ng-template",
                "matthewpi.caddyfile-support"
            ]
        }
    }
}